    "/stream/upload": {
      "post": {
        "tags": [
          "Stream"
        ],
        "summary": "Upload to a remote a single file",
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "magnet": {
                  "example": "any"
                },
                "fileName": {
                  "example": "any"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Result of the operation",
            "schema": {
              "type": "object",
              "properties": {
                "files": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "example": "xxx.mp4"
                      },
                      "size": {
                        "type": "number",
                        "example": 11111
                      },
                      "url": {
                        "type": "string",
                        "example": "https://domain.com/mcurg8n382uy"
                      },
                      "deleteUrl": {
                        "type": "string",
                        "example": "https://domain.com/mcurg8n382uy?killcode=agdt0meepz"
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "404": {
            "description": "Not Found"
          },
          "405": {
            "description": "Method Not Allowed"
          }
        }
      }
    },
