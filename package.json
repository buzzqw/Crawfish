{
  "name": "crawfish",
  "version": "1.7.2",
  "private": false,
  "author": "drakonkat",
  "description": "Innovative torrent client",
  "repository": "https://github.com/drakonkat/crawfish",
  "keywords": [
    "torrent",
    "docker",
    "express",
    "stream"
  ],
  "scripts": {
    "start": "npx nodemon --ignore config.json ./bin/www",
    "start-electron": "electron .",
    "swagger-autogen": "node swagger.js",
    "prod": "node ./bin/www-prod",
    "bundle": "electron-builder",
    "bundle-linux": "electron-builder -l --publish never",
    "bundle-win": "electron-builder --win --publish never",
    "bundleg": "electron-builder -p always"
  },
  "build": {
    "appId": "com.tnd.crawfish",
    "productName": "CrawFish",
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ],
      "requestedExecutionLevel": "requireAdministrator"
    },
    "linux":{
        "target":"AppImage"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./public/build/favicon.ico",
      "uninstallerIcon": "./public/build/favicon.ico"
    },
    "icon": "public/build/logo512.png",
    "copyright": "Copyright 20120-2022 TND"
  },
  "main": "public/electron.js",
  "dependencies": {
    "axios": "^0.25.0",
    "cheerio": "^1.0.0-rc.10",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "downloads-folder": "^3.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.0.5",
    "express": "~4.16.1",
    "fast-xml-parser": "^4.0.7",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "mime-types": "^2.1.35",
    "morgan": "~1.9.1",
    "open": "^8.4.0",
    "pouchdb": "^7.3.0",
    "pug": "^3.0.2",
    "qs": "^6.10.3",
    "rotating-file-stream": "^3.0.4",
    "swagger-ui-express": "^4.2.0",
    "webtorrent": "^1.8.16",
    "webtorrent-hybrid": "^4.1.3"
  },
  "devDependencies": {
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "nodemon": "^2.0.18",
    "swagger-autogen": "^2.21.4"
  }
}
