{
  "name": "webtorrent-express-api",
  "version": "1.5.3",
  "private": true,
  "keywords": [
    "torrent",
    "docker",
    "express",
    "stream"
  ],
  "type": "commonjs",
  "scripts": {
    "start": "npx nodemon --ignore config.json bin/www",
    "start-electron": "electron .",
    "swagger-autogen": "node swagger.js",
    "bundle": "electron-builder"
  },
  "build": {
    "appId": "com.tnd.crawfish",
    "productName": "CrawFish",
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "asar": true,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./public/build/favicon.ico",
      "uninstallerIcon": "./public/build/favicon.ico"
    },
    "icon": "public/build/logo512.png",
    "copyright": "Copyright 20120-2022 TND"
  },
  "main": "public/electron.js",
  "dependencies": {
    "axios": "^0.25.0",
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "downloads-folder": "^3.0.1",
    "electron-is-dev": "^2.0.0",
    "express": "~4.16.1",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "open": "^8.4.0",
    "pug": "^3.0.2",
    "qs": "^6.10.3",
    "swagger-ui-express": "^4.2.0",
    "webtorrent": "^1.8.13",
    "webtorrent-hybrid": "^5.0.2"
  },
  "devDependencies": {
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "nodemon": "^2.0.15",
    "swagger-autogen": "^2.13.2",
    "typescript": "^4.6.3"
  }
}
