{
  "name": "webtorrent-express-api",
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "start": "npx nodemon --ignore config.json ./bin/www",
    "swagger-autogen": "node swagger.js",
    "prod": "node ./bin/www-prod",
    "bundle": "pkg ./bin/www-prod -c \"./package.json\" --out-path \"./build/\""
  },
  "pkg": {
    "scripts": "public/**/*.js",
    "assets": "public/**/*",
    "outputPath": "dist"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "open": "^8.4.0",
    "pug": "^3.0.2",
    "qs": "^6.10.3",
    "swagger-ui-express": "^4.2.0",
    "webtorrent": "^1.5.8",
    "webtorrent-hybrid": "^4.1.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "swagger-autogen": "^2.13.2"
  }
}
